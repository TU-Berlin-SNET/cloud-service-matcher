<?xml version="1.0" encoding="utf-8"  ?>

<Query>
  <QoSAttribute>
    <name>price</name>
    <unit>US dollar</unit>
    <type>range value</type>
    <tendency>negative</tendency>
    <weight>0.2</weight>
    <values>
      <from>100</from>
      <to>150</to>
    </values>
  </QoSAttribute>

  <QoSAttribute>
    <name>responseTime</name>
    <unit>msec</unit>
    <type>range value</type>
    <tendency>negative</tendency>
    <weight>0.4</weight>
    <values>
      <from>0</from>
      <to>5</to>
    </values>
  </QoSAttribute>

  <QoSAttribute>
    <name>availablity</name>
    <unit>%</unit>
    <type>fuzzy value</type>
    <tendency>positive</tendency>
    <weight>0.7</weight>
    <values>
      <value>good</value>
    </values>
  </QoSAttribute>

  <QoSAttribute>
    <name>authentication</name>
    <unit>boolean</unit>
    <type>single value</type>
    <tendency>neutral</tendency>
    <weight>0.5</weight>
    <values>
      <value>yes</value>
    </values>
  </QoSAttribute>

  <requestFeatures>
    <preferences>
      <item>
        <attrName>responseTime</attrName>
        <priority>1</priority>
      </item>
      <item>
        <attrName>price</attrName>
        <priority>2</priority>
      </item>
    </preferences>
    <RelaxationOrder>
      <item>
        <attrName>availablity</attrName>
        <order>1</order>
      </item>    
      <item>
        <attrName>authentication</attrName>
        <order>2</order>
      </item>
    </RelaxationOrder>
    <timeConstraints>
      <startDate>02/17/2010</startDate>
      <endDate>07/17/2010</endDate>
      <frequencyOfUsage>
        <value>3</value>
        <unit>week</unit>
      </frequencyOfUsage>
      <durationOfUsage>
        <value>2</value>
        <unit>hours</unit>
      </durationOfUsage>
    </timeConstraints>      
  </requestFeatures>
</Query>