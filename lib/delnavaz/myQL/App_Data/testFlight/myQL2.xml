<QoSConstraints>
  <QoSConstraint>
    <name>response time</name>
    <values>
      <from>0</from>
      <to>300</to>
    </values>
    <unit>millisecond</unit>
    <type>range</type>
    <tendency>negative</tendency>
    <preference>4</preference>
    <relaxation>1</relaxation>
    <weight>0.025</weight>
  </QoSConstraint>
  <QoSConstraint>
    <name>availability</name>
    <values>
      <from>89</from>
      <to>100</to>
    </values>
    <unit>%</unit>
    <type>range</type>
    <tendency>positive</tendency>
    <preference>2</preference>
    <relaxation>0</relaxation>
    <weight>0.525</weight>
  </QoSConstraint>
  <QoSConstraint>
    <name>latency</name>
    <values>
      <from>0</from>
      <to>16</to>
    </values>
    <unit>%</unit>
    <type>range</type>
    <tendency>negative</tendency>
    <preference>1</preference>
    <relaxation>2</relaxation>
    <weight>0.775</weight>
  </QoSConstraint>
  <QoSConstraint>
    <name>throughput</name>
    <values>
      <from>50</from>
      <to>200</to>
    </values>
    <unit>%</unit>
    <type>range</type>
    <tendency>positive</tendency>
    <preference>3</preference>
    <relaxation>3</relaxation>
    <weight>0.275</weight>
  </QoSConstraint>
</QoSConstraints>