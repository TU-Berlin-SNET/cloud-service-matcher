<QoSConstraints>
  <QoSConstraint>
    <name>response time</name>
    <values>
      <from>0</from>
      <to>600</to>
    </values>
    <unit>millisecond</unit>
    <type>range</type>
    <tendency>negative</tendency>
    <preference>1</preference>
    <relaxation>1</relaxation>
    <weight>0.7</weight>
  </QoSConstraint>
  <QoSConstraint>
    <name>availability</name>
    <values>
      <from>75</from>
      <to>100</to>
    </values>
    <unit>%</unit>
    <type>range</type>
    <tendency>positive</tendency>
    <preference>2</preference>
    <relaxation>0</relaxation>
    <weight>0.366666666666667</weight>
  </QoSConstraint>
  <QoSConstraint>
    <name>latency</name>
    <values>
      <from>0</from>
      <to>12</to>
    </values>
    <unit>%</unit>
    <type>range</type>
    <tendency>negative</tendency>
    <preference>3</preference>
    <relaxation>3</relaxation>
    <weight>0.0333333333333333</weight>
  </QoSConstraint>
  <QoSConstraint>
    <name>throughput</name>
    <values>
      <from>30</from>
      <to>900</to>
    </values>
    <unit>%</unit>
    <type>range</type>
    <tendency>positive</tendency>
    <preference>1</preference>
    <relaxation>2</relaxation>
    <weight>0.7</weight>
  </QoSConstraint>
</QoSConstraints>