<QoSConstraints>
  <QoSConstraint>
    <name>response time</name>
    <values>
      <from>0</from>
      <to>120</to>
    </values>
    <unit>millisecond</unit>
    <type>range</type>
    <tendency>negative</tendency>
    <preference>2</preference>
    <relaxation>0</relaxation>
    <weight>0.525</weight>
  </QoSConstraint>
  <QoSConstraint>
    <name>availability</name>
    <values>
      <from>80</from>
      <to>100</to>
    </values>
    <unit>%</unit>
    <type>range</type>
    <tendency>positive</tendency>
    <preference>1</preference>
    <relaxation>0</relaxation>
    <weight>0.775</weight>
  </QoSConstraint>
  <QoSConstraint>
    <name>throughput</name>
    <values>
      <from>14</from>
      <to>50</to>
    </values>
    <unit>%</unit>
    <type>range</type>
    <tendency>positive</tendency>
    <preference>3</preference>
    <relaxation>1</relaxation>
    <weight>0.275</weight>
  </QoSConstraint>
  <QoSConstraint>
    <name>latency</name>
    <values>
      <from>0</from>
      <to>6</to>
    </values>
    <unit>%</unit>
    <type>range</type>
    <tendency>negative</tendency>
    <preference>4</preference>
    <relaxation>2</relaxation>
    <weight>0.025</weight>
  </QoSConstraint>
</QoSConstraints>