<?xml version="1.0" encoding="utf-8" ?>
<QoSConstraints>
  <QoSConstraint>
    <name>price</name>
    <type>range</type>
    <unit>US dollar</unit>
    <tendency>negative</tendency>
    <preference>1</preference>
    <relaxation>2</relaxation>
    <weight>0.3</weight>
    <values>
      <from>80</from>
      <to>80</to>
    </values>
  </QoSConstraint>
  <QoSConstraint>
    <name>reliability</name>
    <type>range</type>
    <unit>%</unit>
    <tendency>positive</tendency>
    <preference>3</preference>
    <relaxation>0</relaxation>
    <weight>0.1</weight>
    <values>
      <from>0.98</from>
      <to>0.98</to>
    </values>
  </QoSConstraint>
  <QoSConstraint>
    <name>responsetime</name>
    <type>range</type>
    <unit>msec</unit>
    <tendency>negative</tendency>
    <preference>2</preference>
    <relaxation>1</relaxation>
    <weight>0.15</weight>
    <values>
      <from>4</from>
      <to>4</to>
    </values>
  </QoSConstraint>
  <QoSConstraint>
    <name>authentication</name>
    <type>single</type>
    <unit></unit>
    <tendency>neutral</tendency>
    <preference>3</preference>
    <relaxation>3</relaxation>
    <weight>0.15</weight>
    <values>
      <from>true</from>
      <to>true</to>
    </values>
  </QoSConstraint>
</QoSConstraints>