<QoSConstraints>
  <QoSConstraint>
    <name>price</name>
    <values>
      <from>0</from>
      <to>150</to>
    </values>
    <unit>Euro</unit>
    <weight />
    <type>range</type>
    <tendency>negative</tendency>
    <preference />
    <relaxation />
  </QoSConstraint>
  <QoSConstraint>
    <name>reliability</name>
    <values>
      <from>0.95</from>
      <to>0.98</to>
    </values>
    <unit>%</unit>
    <weight />
    <type>fuzzy</type>
    <tendency>positive</tendency>
    <preference />
    <relaxation />
  </QoSConstraint>
  <QoSConstraint>
    <name>response time</name>
    <values>
      <from>0</from>
      <to>250</to>
    </values>
    <unit>millisecond</unit>
    <weight />
    <type>range</type>
    <tendency>negative</tendency>
    <preference />
    <relaxation />
  </QoSConstraint>
  <QoSConstraint>
    <name>authentication</name>
    <values>
      <from>true</from>
      <to>true</to>
    </values>
    <unit>boolean</unit>
    <weight />
    <type>single</type>
    <tendency>neutral</tendency>
    <preference />
    <relaxation />
  </QoSConstraint>
</QoSConstraints>