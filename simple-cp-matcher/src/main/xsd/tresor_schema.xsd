<?xml version="1.0"?>
<ns:schema xmlns="http://www.open-service-compendium.org" xmlns:ns="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.open-service-compendium.org" elementFormDefault="qualified">
  <ns:element name="services">
    <ns:annotation>
      <ns:documentation>A list of services.</ns:documentation>
    </ns:annotation>
    <ns:complexType>
      <ns:sequence>
        <ns:element name="service" type="Service" minOccurs="0" maxOccurs="unbounded"/>
      </ns:sequence>
    </ns:complexType>
  </ns:element>
  <ns:element name="service" type="Service">
    <ns:annotation>
      <ns:documentation>The root element of a service.</ns:documentation>
    </ns:annotation>
  </ns:element>
  <ns:complexType name="SDLTypeBase" abstract="true">
    <ns:annotation>
      <ns:documentation>The base type of all SDL types and facts. Contains annotations and documentation.</ns:documentation>
    </ns:annotation>
    <ns:attribute name="annotation" type="ns:string">
      <ns:annotation>
        <ns:documentation>An arbitrary annotation to be displayed for the description consumer.</ns:documentation>
      </ns:annotation>
    </ns:attribute>
    <ns:attribute name="uri">
      <ns:annotation>
        <ns:documentation>An identifier for a type instance. Two instances are the same, if they share the same identifier.</ns:documentation>
      </ns:annotation>
    </ns:attribute>
  </ns:complexType>
  <ns:complexType name="Service">
    <ns:annotation>
      <ns:documentation>A service</ns:documentation>
    </ns:annotation>
    <ns:complexContent>
      <ns:extension base="SDLTypeBase">
        <ns:sequence>
          <ns:group ref="ServiceProperties"/>
        </ns:sequence>
      </ns:extension>
    </ns:complexContent>
  </ns:complexType>
  <ns:group name="ServiceProperties">
    <ns:sequence>
      <ns:element name="service_name" minOccurs="0" maxOccurs="1">
        <ns:annotation>
          <ns:documentation>Service name</ns:documentation>
        </ns:annotation>
        <ns:complexType>
          <ns:simpleContent>
            <ns:extension base="ns:string">
              <ns:attribute name="annotation"/>
            </ns:extension>
          </ns:simpleContent>
        </ns:complexType>
      </ns:element>
      <ns:element name="provider" type="Provider" minOccurs="0">
        <ns:annotation>
          <ns:documentation>The service provider</ns:documentation>
        </ns:annotation>
      </ns:element>
      <ns:element name="cloud_service_model" type="CloudServiceModel" minOccurs="0">
        <ns:annotation>
          <ns:documentation>The cloud service model</ns:documentation>
        </ns:annotation>
      </ns:element>
      <ns:element name="service_category" type="ServiceCategory" minOccurs="0" maxOccurs="unbounded">
        <ns:annotation>
          <ns:documentation>Categories</ns:documentation>
        </ns:annotation>
      </ns:element>
      <ns:element name="service_tag" minOccurs="0" maxOccurs="unbounded">
        <ns:annotation>
          <ns:documentation>Tags</ns:documentation>
        </ns:annotation>
        <ns:complexType>
          <ns:simpleContent>
            <ns:extension base="ns:string">
              <ns:attribute name="annotation"/>
            </ns:extension>
          </ns:simpleContent>
        </ns:complexType>
      </ns:element>
      <ns:element name="add_on_repository" type="AddOnRepository" minOccurs="0">
        <ns:annotation>
          <ns:documentation>The service add-on repositories</ns:documentation>
        </ns:annotation>
      </ns:element>
      <ns:element name="is_charged_by" type="ChargeUnit" minOccurs="0">
        <ns:annotation>
          <ns:documentation>Charging unit</ns:documentation>
        </ns:annotation>
      </ns:element>
      <ns:element name="data_location" type="Location" minOccurs="0">
        <ns:annotation>
          <ns:documentation>Service data storage location</ns:documentation>
        </ns:annotation>
      </ns:element>
      <ns:element name="data_deletion_policy" minOccurs="0" maxOccurs="1">
        <ns:annotation>
          <ns:documentation>The data deletion policy</ns:documentation>
        </ns:annotation>
        <ns:complexType>
          <ns:simpleContent>
            <ns:extension base="ns:anyURI">
              <ns:attribute name="annotation"/>
            </ns:extension>
          </ns:simpleContent>
        </ns:complexType>
      </ns:element>
      <ns:element name="status_page" minOccurs="0" maxOccurs="1">
        <ns:annotation>
          <ns:documentation>The status page URL</ns:documentation>
        </ns:annotation>
        <ns:complexType>
          <ns:simpleContent>
            <ns:extension base="ns:anyURI">
              <ns:attribute name="annotation"/>
            </ns:extension>
          </ns:simpleContent>
        </ns:complexType>
      </ns:element>
      <ns:element name="public_service_level_agreement" minOccurs="0" maxOccurs="1">
        <ns:annotation>
          <ns:documentation>URL of public SLA</ns:documentation>
        </ns:annotation>
        <ns:complexType>
          <ns:simpleContent>
            <ns:extension base="ns:anyURI">
              <ns:attribute name="annotation"/>
            </ns:extension>
          </ns:simpleContent>
        </ns:complexType>
      </ns:element>
      <ns:element name="data_encryption" type="DataEncryption" minOccurs="0">
        <ns:annotation>
          <ns:documentation>Details about service data encryption</ns:documentation>
        </ns:annotation>
      </ns:element>
      <ns:element name="audit_option" type="AuditOption" minOccurs="0" maxOccurs="unbounded">
        <ns:annotation>
          <ns:documentation>The audit options</ns:documentation>
        </ns:annotation>
      </ns:element>
      <ns:element name="is_billed" type="BillingAndPayment" minOccurs="0">
        <ns:annotation>
          <ns:documentation>Billing and Payment info</ns:documentation>
        </ns:annotation>
      </ns:element>
      <ns:element name="payment_option" type="PaymentOption" minOccurs="0" maxOccurs="unbounded">
        <ns:annotation>
          <ns:documentation>Payment options</ns:documentation>
        </ns:annotation>
      </ns:element>
      <ns:element name="set_up_consumption_duration" minOccurs="0" maxOccurs="1">
        <ns:annotation>
          <ns:documentation>The duration for setting up the service.</ns:documentation>
        </ns:annotation>
        <ns:complexType>
          <ns:simpleContent>
            <ns:extension base="ns:string">
              <ns:attribute name="annotation"/>
            </ns:extension>
          </ns:simpleContent>
        </ns:complexType>
      </ns:element>
      <ns:element name="provision_end_user_duration" minOccurs="0" maxOccurs="1">
        <ns:annotation>
          <ns:documentation>The duration for provisioning an end user.</ns:documentation>
        </ns:annotation>
        <ns:complexType>
          <ns:simpleContent>
            <ns:extension base="ns:string">
              <ns:attribute name="annotation"/>
            </ns:extension>
          </ns:simpleContent>
        </ns:complexType>
      </ns:element>
      <ns:element name="feature" type="Feature" minOccurs="0" maxOccurs="unbounded">
        <ns:annotation>
          <ns:documentation>Service features</ns:documentation>
        </ns:annotation>
      </ns:element>
      <ns:element name="documentation" minOccurs="0" maxOccurs="1">
        <ns:annotation>
          <ns:documentation>Service documentation URL</ns:documentation>
        </ns:annotation>
        <ns:complexType>
          <ns:simpleContent>
            <ns:extension base="ns:anyURI">
              <ns:attribute name="annotation"/>
            </ns:extension>
          </ns:simpleContent>
        </ns:complexType>
      </ns:element>
      <ns:element name="rest_interface" type="RestInterface" minOccurs="0">
        <ns:annotation>
          <ns:documentation>The REST interface</ns:documentation>
        </ns:annotation>
      </ns:element>
      <ns:element name="soap_interface" type="SoapInterface" minOccurs="0">
        <ns:annotation>
          <ns:documentation>The SOAP interface</ns:documentation>
        </ns:annotation>
      </ns:element>
      <ns:element name="xmlrpc_interface" type="XmlrpcInterface" minOccurs="0">
        <ns:annotation>
          <ns:documentation>The XMLRPC interface</ns:documentation>
        </ns:annotation>
      </ns:element>
      <ns:element name="compatible_browser" type="CompatibleBrowser" minOccurs="0" maxOccurs="unbounded">
        <ns:annotation>
          <ns:documentation>Compatible Browsers</ns:documentation>
        </ns:annotation>
      </ns:element>
      <ns:element name="required_plugin" type="RequiredPlugin" minOccurs="0" maxOccurs="unbounded">
        <ns:annotation>
          <ns:documentation>Required Browser-Plugins</ns:documentation>
        </ns:annotation>
      </ns:element>
      <ns:element name="maintenance_free" type="MaintenanceFree" minOccurs="0">
        <ns:annotation>
          <ns:documentation>The service is maintenance free</ns:documentation>
        </ns:annotation>
      </ns:element>
      <ns:element name="maintenance_window" type="MaintenanceWindow" minOccurs="0">
        <ns:annotation>
          <ns:documentation>The service has a maintenance window</ns:documentation>
        </ns:annotation>
      </ns:element>
      <ns:element name="future_roadmap" minOccurs="0" maxOccurs="1">
        <ns:annotation>
          <ns:documentation>Future roadmap URL</ns:documentation>
        </ns:annotation>
        <ns:complexType>
          <ns:simpleContent>
            <ns:extension base="ns:anyURI">
              <ns:attribute name="annotation"/>
            </ns:extension>
          </ns:simpleContent>
        </ns:complexType>
      </ns:element>
      <ns:element name="past_release_notes" minOccurs="0" maxOccurs="1">
        <ns:annotation>
          <ns:documentation>Past release notes URL</ns:documentation>
        </ns:annotation>
        <ns:complexType>
          <ns:simpleContent>
            <ns:extension base="ns:anyURI">
              <ns:attribute name="annotation"/>
            </ns:extension>
          </ns:simpleContent>
        </ns:complexType>
      </ns:element>
      <ns:element name="feedback_page" minOccurs="0" maxOccurs="1">
        <ns:annotation>
          <ns:documentation>Feedback page URL</ns:documentation>
        </ns:annotation>
        <ns:complexType>
          <ns:simpleContent>
            <ns:extension base="ns:anyURI">
              <ns:attribute name="annotation"/>
            </ns:extension>
          </ns:simpleContent>
        </ns:complexType>
      </ns:element>
      <ns:element name="exportable_data_format" type="DataFormat" minOccurs="0" maxOccurs="unbounded">
        <ns:annotation>
          <ns:documentation>Exportable data formats</ns:documentation>
        </ns:annotation>
      </ns:element>
      <ns:element name="importable_data_format" type="DataFormat" minOccurs="0" maxOccurs="unbounded">
        <ns:annotation>
          <ns:documentation>Importable data formats</ns:documentation>
        </ns:annotation>
      </ns:element>
      <ns:element name="is_protected_by" type="CommunicationProtection" minOccurs="0">
        <ns:annotation>
          <ns:documentation>Service protection means</ns:documentation>
        </ns:annotation>
      </ns:element>
      <ns:element name="can_be_used_offline" minOccurs="0" maxOccurs="1">
        <ns:annotation>
          <ns:documentation>Offline capability information</ns:documentation>
        </ns:annotation>
        <ns:complexType>
          <ns:simpleContent>
            <ns:extension base="ns:boolean">
              <ns:attribute name="annotation"/>
            </ns:extension>
          </ns:simpleContent>
        </ns:complexType>
      </ns:element>
      <ns:element name="established_in" minOccurs="0" maxOccurs="1">
        <ns:annotation>
          <ns:documentation>Service established in</ns:documentation>
        </ns:annotation>
        <ns:complexType>
          <ns:simpleContent>
            <ns:extension base="ns:decimal">
              <ns:attribute name="annotation"/>
            </ns:extension>
          </ns:simpleContent>
        </ns:complexType>
      </ns:element>
      <ns:element name="location_enabled" minOccurs="0" maxOccurs="1">
        <ns:annotation>
          <ns:documentation>The service supports location-based computing</ns:documentation>
        </ns:annotation>
        <ns:complexType>
          <ns:simpleContent>
            <ns:extension base="ns:boolean">
              <ns:attribute name="annotation"/>
            </ns:extension>
          </ns:simpleContent>
        </ns:complexType>
      </ns:element>
    </ns:sequence>
  </ns:group>
  <ns:complexType name="Timespan">
    <ns:annotation>
      <ns:documentation>An arbitrary timespan</ns:documentation>
    </ns:annotation>
    <ns:complexContent>
      <ns:extension base="SDLTypeBase">
        <ns:sequence>
          <ns:group ref="TimespanProperties"/>
        </ns:sequence>
        <ns:attribute name="identifier" type="TimespanIdentifier">
          <ns:annotation>
            <ns:documentation>An identifier for a type instance. Two instances are the same, if they share the same identifier.</ns:documentation>
          </ns:annotation>
        </ns:attribute>
      </ns:extension>
    </ns:complexContent>
  </ns:complexType>
  <ns:group name="TimespanProperties">
    <ns:sequence>
      <ns:element name="start" minOccurs="0" maxOccurs="1">
        <ns:annotation>
          <ns:documentation>The timespan start</ns:documentation>
        </ns:annotation>
        <ns:complexType>
          <ns:simpleContent>
            <ns:extension base="ns:string">
              <ns:attribute name="annotation"/>
            </ns:extension>
          </ns:simpleContent>
        </ns:complexType>
      </ns:element>
      <ns:element name="end" minOccurs="0" maxOccurs="1">
        <ns:annotation>
          <ns:documentation>The timespan end</ns:documentation>
        </ns:annotation>
        <ns:complexType>
          <ns:simpleContent>
            <ns:extension base="ns:string">
              <ns:attribute name="annotation"/>
            </ns:extension>
          </ns:simpleContent>
        </ns:complexType>
      </ns:element>
    </ns:sequence>
  </ns:group>
  <ns:simpleType name="TimespanIdentifier">
    <ns:restriction base="ns:string"/>
  </ns:simpleType>
  <ns:complexType name="Provider">
    <ns:annotation>
      <ns:documentation>A service provider</ns:documentation>
    </ns:annotation>
    <ns:complexContent>
      <ns:extension base="SDLTypeBase">
        <ns:sequence>
          <ns:group ref="ProviderProperties"/>
        </ns:sequence>
        <ns:attribute name="identifier" type="ProviderIdentifier">
          <ns:annotation>
            <ns:documentation>An identifier for a type instance. Two instances are the same, if they share the same identifier.</ns:documentation>
          </ns:annotation>
        </ns:attribute>
      </ns:extension>
    </ns:complexContent>
  </ns:complexType>
  <ns:group name="ProviderProperties">
    <ns:sequence>
      <ns:element name="provider_name" minOccurs="0" maxOccurs="1">
        <ns:annotation>
          <ns:documentation>Name</ns:documentation>
        </ns:annotation>
        <ns:complexType>
          <ns:simpleContent>
            <ns:extension base="ns:string">
              <ns:attribute name="annotation"/>
            </ns:extension>
          </ns:simpleContent>
        </ns:complexType>
      </ns:element>
      <ns:element name="provider_location" type="SpecificLocation" minOccurs="0">
        <ns:annotation>
          <ns:documentation>Location</ns:documentation>
        </ns:annotation>
      </ns:element>
      <ns:element name="company_type" type="CompanyType" minOccurs="0">
        <ns:annotation>
          <ns:documentation>Company type</ns:documentation>
        </ns:annotation>
      </ns:element>
      <ns:element name="employs" minOccurs="0" maxOccurs="1">
        <ns:annotation>
          <ns:documentation>Number of employees</ns:documentation>
        </ns:annotation>
        <ns:complexType>
          <ns:simpleContent>
            <ns:extension base="ns:decimal">
              <ns:attribute name="annotation"/>
            </ns:extension>
          </ns:simpleContent>
        </ns:complexType>
      </ns:element>
      <ns:element name="partner_network" minOccurs="0" maxOccurs="1">
        <ns:annotation>
          <ns:documentation>Partner network</ns:documentation>
        </ns:annotation>
        <ns:complexType>
          <ns:simpleContent>
            <ns:extension base="ns:anyURI">
              <ns:attribute name="annotation"/>
            </ns:extension>
          </ns:simpleContent>
        </ns:complexType>
      </ns:element>
      <ns:element name="reference_customer" type="ReferenceCustomer" minOccurs="0" maxOccurs="unbounded">
        <ns:annotation>
          <ns:documentation>Reference customers</ns:documentation>
        </ns:annotation>
      </ns:element>
      <ns:element name="last_years_revenue" minOccurs="0" maxOccurs="1">
        <ns:annotation>
          <ns:documentation>Last years revenue</ns:documentation>
        </ns:annotation>
        <ns:complexType>
          <ns:simpleContent>
            <ns:extension base="ns:string">
              <ns:attribute name="annotation"/>
            </ns:extension>
          </ns:simpleContent>
        </ns:complexType>
      </ns:element>
      <ns:element name="initiative" type="Initiative" minOccurs="0" maxOccurs="unbounded">
        <ns:annotation>
          <ns:documentation>Participatory initiatives</ns:documentation>
        </ns:annotation>
      </ns:element>
      <ns:element name="report" type="ReportingInformation" minOccurs="0" maxOccurs="unbounded">
        <ns:annotation>
          <ns:documentation>Published reports</ns:documentation>
        </ns:annotation>
      </ns:element>
    </ns:sequence>
  </ns:group>
  <ns:simpleType name="ProviderIdentifier">
    <ns:restriction base="ns:string"/>
  </ns:simpleType>
  <ns:complexType name="Jurisdiction">
    <ns:annotation>
      <ns:documentation>A jurisdictional area</ns:documentation>
    </ns:annotation>
    <ns:complexContent>
      <ns:extension base="SDLTypeBase">
        <ns:sequence>
          <ns:group ref="JurisdictionProperties"/>
        </ns:sequence>
        <ns:attribute name="identifier" type="JurisdictionIdentifier">
          <ns:annotation>
            <ns:documentation>An identifier for a type instance. Two instances are the same, if they share the same identifier.</ns:documentation>
          </ns:annotation>
        </ns:attribute>
      </ns:extension>
    </ns:complexContent>
  </ns:complexType>
  <ns:group name="JurisdictionProperties">
    <ns:sequence/>
  </ns:group>
  <ns:simpleType name="JurisdictionIdentifier">
    <ns:restriction base="ns:string">
      <ns:enumeration value="european_union">
        <ns:annotation>
          <ns:documentation>European Union</ns:documentation>
        </ns:annotation>
      </ns:enumeration>
      <ns:enumeration value="safe_harbour">
        <ns:annotation>
          <ns:documentation>Safe Harbour</ns:documentation>
        </ns:annotation>
      </ns:enumeration>
    </ns:restriction>
  </ns:simpleType>
  <ns:complexType name="Country">
    <ns:annotation>
      <ns:documentation>A country</ns:documentation>
    </ns:annotation>
    <ns:complexContent>
      <ns:extension base="SDLTypeBase">
        <ns:sequence>
          <ns:group ref="CountryProperties"/>
        </ns:sequence>
        <ns:attribute name="identifier" type="CountryIdentifier">
          <ns:annotation>
            <ns:documentation>An identifier for a type instance. Two instances are the same, if they share the same identifier.</ns:documentation>
          </ns:annotation>
        </ns:attribute>
      </ns:extension>
    </ns:complexContent>
  </ns:complexType>
  <ns:group name="CountryProperties">
    <ns:sequence>
      <ns:element name="jurisdiction" type="Jurisdiction" minOccurs="0">
        <ns:annotation>
          <ns:documentation>The jurisdictions the country falls under</ns:documentation>
        </ns:annotation>
      </ns:element>
    </ns:sequence>
  </ns:group>
  <ns:simpleType name="CountryIdentifier">
    <ns:restriction base="ns:string">
      <ns:enumeration value="germany">
        <ns:annotation>
          <ns:documentation>Germany</ns:documentation>
        </ns:annotation>
      </ns:enumeration>
    </ns:restriction>
  </ns:simpleType>
  <ns:complexType name="Address">
    <ns:annotation>
      <ns:documentation>A postal address</ns:documentation>
    </ns:annotation>
    <ns:complexContent>
      <ns:extension base="SDLTypeBase">
        <ns:sequence>
          <ns:group ref="AddressProperties"/>
        </ns:sequence>
        <ns:attribute name="identifier" type="AddressIdentifier">
          <ns:annotation>
            <ns:documentation>An identifier for a type instance. Two instances are the same, if they share the same identifier.</ns:documentation>
          </ns:annotation>
        </ns:attribute>
      </ns:extension>
    </ns:complexContent>
  </ns:complexType>
  <ns:group name="AddressProperties">
    <ns:sequence>
      <ns:element name="address_line" minOccurs="0" maxOccurs="unbounded">
        <ns:annotation>
          <ns:documentation>The address lines</ns:documentation>
        </ns:annotation>
        <ns:complexType>
          <ns:simpleContent>
            <ns:extension base="ns:string">
              <ns:attribute name="annotation"/>
            </ns:extension>
          </ns:simpleContent>
        </ns:complexType>
      </ns:element>
    </ns:sequence>
  </ns:group>
  <ns:simpleType name="AddressIdentifier">
    <ns:restriction base="ns:string"/>
  </ns:simpleType>
  <ns:complexType name="Location">
    <ns:annotation>
      <ns:documentation>Location information</ns:documentation>
    </ns:annotation>
    <ns:complexContent>
      <ns:extension base="SDLTypeBase">
        <ns:sequence>
          <ns:group ref="LocationProperties"/>
        </ns:sequence>
        <ns:attribute name="identifier" type="LocationIdentifier">
          <ns:annotation>
            <ns:documentation>An identifier for a type instance. Two instances are the same, if they share the same identifier.</ns:documentation>
          </ns:annotation>
        </ns:attribute>
      </ns:extension>
    </ns:complexContent>
  </ns:complexType>
  <ns:group name="LocationProperties">
    <ns:sequence/>
  </ns:group>
  <ns:simpleType name="LocationIdentifier">
    <ns:restriction base="ns:string"/>
  </ns:simpleType>
  <ns:complexType name="SpecificLocation">
    <ns:annotation>
      <ns:documentation>Specific location information</ns:documentation>
    </ns:annotation>
    <ns:complexContent>
      <ns:extension base="SDLTypeBase">
        <ns:sequence>
          <ns:group ref="SpecificLocationProperties"/>
          <ns:group ref="LocationProperties"/>
        </ns:sequence>
        <ns:attribute name="identifier" type="SpecificLocationIdentifier">
          <ns:annotation>
            <ns:documentation>An identifier for a type instance. Two instances are the same, if they share the same identifier.</ns:documentation>
          </ns:annotation>
        </ns:attribute>
      </ns:extension>
    </ns:complexContent>
  </ns:complexType>
  <ns:group name="SpecificLocationProperties">
    <ns:sequence>
      <ns:element name="country" type="Country" minOccurs="0">
        <ns:annotation>
          <ns:documentation>Country</ns:documentation>
        </ns:annotation>
      </ns:element>
      <ns:element name="address" type="Address" minOccurs="0">
        <ns:annotation>
          <ns:documentation>Postal address</ns:documentation>
        </ns:annotation>
      </ns:element>
    </ns:sequence>
  </ns:group>
  <ns:simpleType name="SpecificLocationIdentifier">
    <ns:restriction base="LocationIdentifier"/>
  </ns:simpleType>
  <ns:complexType name="UnspecificLocation">
    <ns:annotation>
      <ns:documentation>Unspecific location information</ns:documentation>
    </ns:annotation>
    <ns:complexContent>
      <ns:extension base="SDLTypeBase">
        <ns:sequence>
          <ns:group ref="UnspecificLocationProperties"/>
          <ns:group ref="LocationProperties"/>
        </ns:sequence>
        <ns:attribute name="identifier" type="UnspecificLocationIdentifier">
          <ns:annotation>
            <ns:documentation>An identifier for a type instance. Two instances are the same, if they share the same identifier.</ns:documentation>
          </ns:annotation>
        </ns:attribute>
      </ns:extension>
    </ns:complexContent>
  </ns:complexType>
  <ns:group name="UnspecificLocationProperties">
    <ns:sequence>
      <ns:element name="jurisdiction" type="Jurisdiction" minOccurs="0">
        <ns:annotation>
          <ns:documentation>The jurisdiction in which the service operates</ns:documentation>
        </ns:annotation>
      </ns:element>
    </ns:sequence>
  </ns:group>
  <ns:simpleType name="UnspecificLocationIdentifier">
    <ns:restriction base="LocationIdentifier"/>
  </ns:simpleType>
  <ns:complexType name="CloudServiceModel">
    <ns:annotation>
      <ns:documentation>A cloud service model</ns:documentation>
    </ns:annotation>
    <ns:complexContent>
      <ns:extension base="SDLTypeBase">
        <ns:sequence>
          <ns:group ref="CloudServiceModelProperties"/>
        </ns:sequence>
        <ns:attribute name="identifier" type="CloudServiceModelIdentifier">
          <ns:annotation>
            <ns:documentation>An identifier for a type instance. Two instances are the same, if they share the same identifier.</ns:documentation>
          </ns:annotation>
        </ns:attribute>
      </ns:extension>
    </ns:complexContent>
  </ns:complexType>
  <ns:group name="CloudServiceModelProperties">
    <ns:sequence/>
  </ns:group>
  <ns:simpleType name="CloudServiceModelIdentifier">
    <ns:restriction base="ns:string">
      <ns:enumeration value="saas">
        <ns:annotation>
          <ns:documentation>Software as a Service (SaaS)</ns:documentation>
        </ns:annotation>
      </ns:enumeration>
      <ns:enumeration value="paas">
        <ns:annotation>
          <ns:documentation>Platform as a Service (PaaS)</ns:documentation>
        </ns:annotation>
      </ns:enumeration>
